type UserResult = 
 variant {
   Err: Errors;
   Ok: User;
 };
type User = 
 record {
   name: opt text;
   surname: opt text;
   username: text;
 };
type Errors = 
 variant {
   InvalidOperator;
   TokenNotExist;
   Unauthorized;
   UserAlreadyExists;
   UserDoesNotExists;
 };
service : {
  createUser: (opt text, opt text, text) -> (UserResult);
  getUser: (principal) -> (opt User) query;
  getUsers: () -> (vec principal) query;
  greet: (text) -> (text);
  greet2: (text) -> (text);
}
